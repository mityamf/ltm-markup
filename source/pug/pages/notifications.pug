extends ../base/layout

block variables
  - var title = "LK-Exhibitor Account"
  - var hasHeader = true
  - var hasFooter = true

block main
  include ../data/support-contacts
  include ../data/notifications

  main.container.container--no-padding
    +renderHotelHeadline(hotelHeadline.title, hotelHeadline.logo, hotelHeadline.note, hotelHeadline.logoWidth, hotelHeadline.logoHeight)
    .notifications()
      +renderHeadlineWithBackButton("Notifications", "#")
      +renderNotifications(notifications)
      +renderOpenMoreBlock("All Notifications")
    +renderSupportContacts(contactsList)
